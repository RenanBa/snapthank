
<% if current_user %>
<div class="user_name">
  <p><span><%= current_user.name %></span> | <%= link_to 'Log Out', logout_path, method: :delete %></p>
</div>
<% end %>

<div class="container">
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>">
      <p class="flash"><%= value %></p>
    </div>
  <% end %>
</div>


